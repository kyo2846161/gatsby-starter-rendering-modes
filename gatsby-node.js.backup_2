const path = require('path')

exports.onCreateWebpackConfig = ({ stage, loaders, actions }) => {
    if (stage === "build-html") {
      actions.setWebpackConfig({
        module: {
          rules: [
            {
              test: /useWindowDimensions.js?$/,
            //   component: path.resolve('src/components/useWindowDimensions.js'),
              use: loaders.null(),
            },
          ],
        },
      })
    }
  }